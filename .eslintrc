{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["google", "eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["warn", "windows"],
    "quotes": ["warn", "double"],
    "semi": ["error", "always"],
    "array-bracket-spacing": ["error", "never"],
    "object-curly-spacing": ["error", "always"],
    "no-console": "warn",
    "one-var": ["error", "always"],
    "no-undef": "off",
    "require-jsdoc": "off"
  },
  "plugins": ["prettier"]
}
